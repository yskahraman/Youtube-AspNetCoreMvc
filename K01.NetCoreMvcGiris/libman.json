{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.4.1",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery",
      "files": [ "jquery.min.js" ]
    },
    {
      "library": "popper.js@1.16.1",
      "destination": "wwwroot/lib/popper",
      "files": [ "esm/popper.min.js" ]
    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jqueryValidate",
      "files": [ "jquery.validate.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jqueryValUnob",
      "files": ["jquery.validate.unobtrusive.min.js"]
    }
  ]
}